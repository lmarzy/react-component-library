{
  "name": "@lmarsden/react-component-library",
  "version": "0.8.0",
  "description": "React component library built with React, Typescript and Rollup",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "types": "dist/typings/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist storybook-static",
    "start": "start-storybook -p 6006",
    "build": "npm run build:js && npm run build:types",
    "build:js": "rollup -c rollup.config.js",
    "build:types": "tsc -p tsconfig.build.json",
    "build-storybook": "build-storybook",
    "test:unit": "jest",
    "format:check": "prettier './src/**/*.{ts,tsx}' --check",
    "format:fix": "prettier './src/**/*.{ts,tsx}' --write",
    "lint": "eslint src/**",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmarzy/react-component-library.git"
  },
  "keywords": [],
  "author": "Lee Marsden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lmarzy/react-component-library/issues"
  },
  "homepage": "https://github.com/lmarzy/react-component-library#readme",
  "peerDependencies": {
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "prettier": "^2.2.1",
    "rollup": "^2.50.6",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "ts-jest": "^27.0.2",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  }
}
